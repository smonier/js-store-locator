{
  "name": "@smonier/js-store-locator",
  "version": "1.0.0",
  "description": "Jahia JS Module - Store locator",
  "keywords": [
    "jahia",
    "module",
    "store-locator"
  ],
  "author": "Stephane Monier <smonier@jahia.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/smonier/js-store-locator.git"
  },
  "type": "module",
  "bugs": {
    "url": "https://github.com/smonier/js-store-locator/issues"
  },
  "homepage": "https://github.com/smonier/js-store-locator",
  "files": [
    "dist",
    "javascript",
    "src/**/*.cnd",
    "src/main/resources",
    "settings",
    "static"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build && run package",
    "clean": "rm -rf dist/",
    "deploy": "jahia-deploy",
    "dev": "vite build --watch",
    "format": "prettier --write --list-different .",
    "lint": "eslint .",
    "package": "yarn pack --out dist/package.tgz",
    "watch": "run dev",
    "watch:callback": "run package && run deploy"
  },
  "devDependencies": {
    "@babel/runtime": "^7.25.0",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@jahia/javascript-modules-library": "^1.0.1",
    "@jahia/ui-extender": "^1.2.0",
    "@jahia/vite-federation-plugin": "^0.1.0",
    "@jahia/vite-plugin": "^1.0.1",
    "@types/leaflet": "^1.9.12",
    "@types/prop-types": "^15",
    "@types/react": "^18.3.12",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.460.0",
    "prettier": "^3.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "^4.2.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^7.0.0"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=22.0.0"
  },
  "jahia": {
    "name": "@smonier/js-store-locator",
    "snapshot": false,
    "module-dependencies": "default",
    "module-type": "module",
    "server": "dist/server/index.js",
    "static-resources": "/dist/client,/dist/assets,/javascript/apps",
    "maven": {
      "groupId": "org.jahia.modules.se.javascript",
      "distributionManagement": {
        "repository": {
          "id": "jahia-releases",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-releases"
        },
        "snapshotRepository": {
          "id": "jahia-snapshots",
          "url": "https://devtools.jahia.com/nexus/content/repositories/jahia-snapshots"
        }
      }
    }
  },
  "dependencies": {
    "@jahia/data-helper": "^1.1.14",
    "@jahia/ui-extender": "^1.2.0",
    "@jahia/vite-federation-plugin": "^0.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0"
  }
}
